    name: project

spec:
   replicas: 2
   selector:
     matchLabels:
       env: dev

   template:
     metadata:
       name: projectpod
       labels:
         env: dev

     spec:
       containers:
	 - name: c1
	   image: omkar1311/spring:1.0
	   ports:
             - containerPorts: 8080
